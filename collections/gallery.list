<script src="/scripts/gallery.js" type="text/javascript"></script>


<div id="slideshow" class="next-slide">
	{.repeated section items}
		<div class="slide content-fill"><img {@|image-meta} alt="{.section body}{@|safe}{.end}" /></div>
	{.end}
</div>


<div id="thumbnails">
	{.repeated section items}
		<img src="{fullUrl}?format=100w" height="100%" {@|image-meta} alt="{.section body}{@}{.end}" />
	{.end}
</div>